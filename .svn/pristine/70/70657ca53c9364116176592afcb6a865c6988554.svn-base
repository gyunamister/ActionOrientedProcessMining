package org.processmining.actionorientedprocessmining.actionengine;

import java.util.Map;

public class Transaction {
	public String op;
	public Map<String, String> vmap;
	
	public Transaction(String op, Map<String, String> vmap) {
		this.op=op;
		this.vmap=vmap;
	}
	
	public String getOp() {
		return this.op;
	}
	
	public Map<String, String> getPmap() {
		return this.vmap;
	}
	
	
	public String toString() {
		return this.op + this.vmap;
	}
}
